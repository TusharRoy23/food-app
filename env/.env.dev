APP_PORT=3000
DOMAIN_URL=http://localhost:3000/

DB_TYPE=postgres
POSTGRES_HOST=food-app-db
POSTGRES_PORT=5432
POSTGRES_PORT_OUTSIDE=3200
POSTGRES_USER=fooduser
POSTGRES_PASSWORD=fooduser

DATABASE_NAME=fooddb
DATABASE_USERNAME=fooduser
DATABASE_PASSWORD=fooduser
DATABASE_SYNC=false
SYNCHRONIZE=true


PGADMIN_DEFAULT_EMAIL=postgres@mail.com
PGADMIN_DEFAULT_PASSWORD=admin12
PGADMIN_LISTEN_PORT=8000
WEBSERVER_PORT=8088

JWT_ACCESS_TOKEN_SECRET=anyKey
JWT_ACCESS_TOKEN_EXPIRATION_TIME=9000000

JWT_REFRESH_TOKEN_SECRET=anyRefreshKey
JWT_REFRESH_TOKEN_EXPIRATION_TIME=1800000

COMPOSE_IGNORE_ORPHANS=True
COMPOSE_HTTP_TIMEOUT=100

ELK_VERSION=7.17.7
ELASTICSEARCH_NODE=http://foodes01:9200
ELASTICSEARCH_USERNAME=elastic
ELASTICSEARCH_PASSWORD=foodapp
node.name=foodes01
cluster.name=es-cluster
ES_JAVA_OPTS="-Xms512m -Xmx512m"
bootstrap.memory_lock=true
discovery.type=single-node
xpack.security.enabled=true

REDIS_VERSION=latest
REDIS_PORT=6379
REDIS_HOST=redis-stack-server
REDIS_PASSWORD=food-stack
REDIS_ARGS="--requirepass ${REDIS_PASSWORD}"